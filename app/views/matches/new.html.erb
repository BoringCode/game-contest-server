<div class='col-md-offset-3 col-md-6'>

    <h1>Challenge in <%= link_to @contest.name, @contest %></h1>
    <br/>
    <%= form_for [@match.manager, @match] do |f| %>
	<%= render 'shared/error_messages', object: @match %>
        <%= render 'fields', f: f %>
        
        <%= f.submit 'Challenge!', class: 'btn btn-primary' %>
    <% end %>

</div>
