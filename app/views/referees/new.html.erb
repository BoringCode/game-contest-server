<h1>Create Referee</h1>

<%= form_for @referee do |f| %>
    <%= render 'shared/error_messages', object: @referee %>

    <div class="form-group">
	<%= f.label :programming_language_id, 'Programming Language' %>
	<%= f.collection_select(:programming_language_id,
				ProgrammingLanguage.all,
				:id,
				:name,
				{},
				class: 'form-control') %>
    </div>

    <div class="form-group">
	<%= f.label :code, 'Upload file' %>
	<%= f.file_field :code, class: 'form-control' %>
    </div>

    <%= f.submit 'Create Referee', class: 'btn btn-primary' %>
<% end %>
