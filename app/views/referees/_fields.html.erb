<div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :rules_url %>
    <%= f.text_field :rules_url, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :match_limit %>
    <%= f.text_field :match_limit, class: 'form-control', :value=>'100' %>
</div>

<div class="form-group">
    <%= f.check_box :rounds_capable, :checked_value => true, :unchecked_value => false%>
    <%= f.label :rounds_capable, 'Referee is capable of handling rounds' %>
</div>

<div class="form-group">
    <%= f.label :players_per_game %>
    <%= f.select :players_per_game, 1..10, {}, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :time_per_game %>
    <%= f.select :time_per_game, 1..15, {}, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :upload, 'Upload file' %>
    <%= f.file_field :upload %>
</div>

<div class="form-group">
    <%= f.label :upload2, 'Upload more files' %>
    <%= f.file_field :upload2 %>
</div>
