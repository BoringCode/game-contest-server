<div class='col-md-offset-3 col-md-6'>
    <h1>New Player <!--for <%#= link_to @player.contest.name, @player.contest %>--></h1>

    <%#= form_for [@player.contest, @player] do |f| %>
    <%= form_for @player do |f| %>
        <%= render 'shared/error_messages', object: @player %>
				<div class="form-group">
    				<%= f.label :contest_id, 'Contest' %>
    				<%= f.select :contest_id, options_from_collection_for_select(@contests, 'id', 'name'), class: 'form-control' %>
				</div>
        <%= render 'fields', f: f %>

        <%= f.submit 'Create Player', class: 'btn btn-primary' %>
    <% end %>
</div>
